{
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {

  },
  "rules": {
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true, "ImportDeclaration": true } }],
    "key-spacing": [2, { "align": "colon" }],
    "no-param-reassign": [2, {"props": false}],
    "arrow-body-style": [0]
  }
}
